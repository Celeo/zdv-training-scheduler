{
  "name": "zdv_training_scheduler",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build_": "astro check && tsc --noEmit && astro build",
    "build": "astro build",
    "astro": "astro",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro}\"",
    "docker:build": "docker build -t celeo/zdv-training-scheduler .",
    "docker:push": "docker push celeo/zdv-training-scheduler:latest",
    "script:import_roster": "tsx scripts/import_roster.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/node": "^6.0.0",
    "@astrojs/react": "^3.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "@prisma/client": "^5.3.1",
    "astro": "^3.1.0",
    "axios": "^1.5.0",
    "jose": "^4.14.6",
    "luxon": "^3.4.3",
    "memoizee": "^0.4.15",
    "nanoid": "^5.0.1",
    "nanostores": "^0.9.3",
    "nodemailer": "^6.9.5",
    "oauth": "^0.10.0",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "toml": "^3.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.2",
    "@types/memoizee": "^0.4.8",
    "@types/nodemailer": "^6.4.11",
    "@types/oauth": "^0.9.2",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-organize-imports"
    ]
  }
}
